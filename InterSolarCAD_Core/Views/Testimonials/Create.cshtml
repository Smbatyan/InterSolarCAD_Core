@model InterSolarCAD_Core.Models.Admin.Entity.Testimonials


<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <div class="panel panel-success">
            <div class="panel-heading">
                <h3>Create Feedback</h3>
            </div>
            <div class="panel-body">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="Company" class="control-label"></label>
                        <input asp-for="Company" class="form-control" />
                        <span asp-validation-for="Company" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="ShortDesc" class="control-label"></label>
                        <textarea asp-for="ShortDesc" class="form-control" rows="6" style="resize: vertical;"></textarea>
                        <span asp-validation-for="ShortDesc" class="text-danger"></span>
                    </div>
                    <input type="submit" value="Create" class="btn btn-w-m btn-primary" />
                </form>
            </div>
        </div>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-w-m btn-default btn-outline"> &lt; Back to list</a>
</div>
